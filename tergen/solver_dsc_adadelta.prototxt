#net: "mnist_autoencoder.prototxt"
#net: "train_test.prototxt"
net: "gan_train_dsc.prototxt"
base_lr: 1.0
lr_policy: "fixed"
momentum: 0.95
delta: 1e-8
display: 100
max_iter: 50000
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "snapshots/out_dsc"
# solver mode: CPU or GPU
solver_mode: GPU
type: "AdaDelta"
