#net: "mnist_autoencoder.prototxt"
#net: "train_test.prototxt"
net: "train_test_euc.prototxt"
base_lr: 1.0
lr_policy: "fixed"
momentum: 0.95
delta: 1e-8
display: 10
max_iter: 60000
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "snapshots/out"
# solver mode: CPU or GPU
solver_mode: GPU
type: "AdaDelta"
