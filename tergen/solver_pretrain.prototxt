#net: "mnist_autoencoder.prototxt"
#net: "train_test.prototxt"
net: "gan_pretrain.prototxt"
base_lr: 1.0
lr_policy: "fixed"
momentum: 0.95
delta: 1e-8
display: 100
max_iter: 1000
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "snapshots/pretrain"
# solver mode: CPU or GPU
solver_mode: GPU
type: "AdaDelta"
